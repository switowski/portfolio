{
  "name": "switowski",
  "version": "1.0.0",
  "description": "Personal website of Sebastian Witowski.",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm-run-all -p build:*",
    "rebuild": "cross-env NODE_ENV=production npm-run-all clean -p build:*",
    "build:11ty": "eleventy --quiet",
    "build:css": "tailwindcss -i src/_assets/styles/main.pcss -o dist/assets/main.bundle.css --minify --postcss",
    "build:js": "esbuild src/_assets/scripts/main.js --outfile=dist/assets/main.bundle.js --bundle",
    "clean": "rm -rf dist",
    "debug": "DEBUG=Eleventy* npm run dev",
    "dev": "cross-env NODE_ENV=development npm-run-all -p dev:*",
    "staging": "cross-env NODE_ENV=production npm-run-all -p dev:*",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:css": "tailwindcss -i src/_assets/styles/main.pcss -o dist/assets/main.bundle.css --watch --postcss",
    "dev:js": "esbuild src/_assets/scripts/main.js --outfile=dist/assets/main.bundle.js --bundle --watch",
    "test": "npx percy snapshot snapshots.local.yml"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/switowski/portfolio.git"
  },
  "keywords": [
    "tailwind",
    "eleventy",
    "alpine"
  ],
  "author": "Sebastian Witowski",
  "license": "CC-BY-NC-SA-4.0",
  "homepage": "https://switowski.com",
  "dependencies": {
    "alpinejs": "^3.1.0",
    "eleventy-plugin-time-to-read": "^1.2.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-img": "^2.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@percy/cli": "^1.10.4",
    "autoprefixer": "^10.4.4",
    "clean-css": "^5.3.1",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.5",
    "eleventy-plugin-broken-links": "^2.0.3",
    "eleventy-plugin-error-overlay": "github:stevenpetryk/eleventy-plugin-error-overlay",
    "eleventy-plugin-feathericons": "^1.0.1",
    "eleventy-plugin-svg-sprite": "^1.3.1",
    "esbuild": "^0.14.3",
    "luxon": "^2.3.1",
    "markdown-it-anchor": "^8.6.4",
    "mocha": "^10.0.0",
    "netlify-plugin-cache": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-custom-properties": "^12.1.5",
    "postcss-extend-rule": "^4.0.0",
    "postcss-import": "^14.0.0",
    "puppeteer": "^18.0.5",
    "tailwindcss": "^3.0.1",
    "tailwindcss-debug-screens": "^2.2.1",
    "terser": "^5.15.0"
  }
}
